{
  "name": "js-utils-lc",
  "version": "0.0.5",
  "private": false,
  "description": "JavaScript工具函数库文档及npm管理",
  "main": "npm/cjs/index.js",
  "module": "npm/es/index.js",
  "sideEffects": false,
  "files": [
    "npm"
  ],
  "scripts": {
    "serve": "gulp serve",
    "dev": "gulp serve",
    "build:doc": "rimraf docs && jsdoc -c jsdoc.json",
    "build:npmdev": "rimraf npm && rollup -c --environment NODE_ENV:development",
    "build:npmprod": "rimraf npm && rollup -c  --environment NODE_ENV:production",
    "test:use": "rollup -c --environment NODE_ENV:development && nodemon npm/cjs/index.js",
    "rm": "rimraf npm && rimraf docs",
    "prepublishOnly": "npm version patch"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/have-not-BUG/js-utils.git"
  },
  "keywords": [
    "js",
    "js utils",
    "JavaScript utils",
    "utils jsdoc",
    "utils",
    "jsdoc hotupdate liveupdate"
  ],
  "author": "have-not-BUG",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/have-not-BUG/js-utils/issues"
  },
  "homepage": "https://github.com/have-not-BUG/js-utils#readme",
  "dependencies": {},
  "devDependencies": {
    "core-js": "~3.15.2",
    "@babel/core": "~7.14.6",
    "@babel/plugin-transform-runtime": "~7.14.5",
    "@babel/preset-env": "~7.14.7",
    "@babel/runtime": "~7.14.6",
    "@babel/runtime-corejs3": "~7.14.7",
    "@rollup/plugin-babel": "~5.3.0",
    "@rollup/plugin-node-resolve": "~13.0.2",
    "babel-plugin-external-helpers": "~6.22.0",
    "babel-preset-latest": "~6.24.1",
    "del": "^2.2.2",
    "docdash": "^1.2.0",
    "glob": "~7.1.7",
    "gulp": "^3.9.1",
    "gulp-connect": "^5.0.0",
    "gulp-jsdoc3": "^1.0.1",
    "jsdoc": "^3.6.4",
    "nodemon": "~2.0.12",
    "rimraf": "~3.0.2",
    "rollup": "~2.53.1",
    "rollup-plugin-commonjs": "~10.1.0",
    "rollup-plugin-json": "~4.0.0",
    "rollup-plugin-livereload": "~2.0.5",
    "rollup-plugin-serve": "~1.1.0",
    "rollup-plugin-terser": "~7.0.2",
    "cheerio": "^0.22.0"
  }
}
